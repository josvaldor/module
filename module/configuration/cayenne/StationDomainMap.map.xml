<?xml version="1.0" encoding="utf-8"?>
<data-map xmlns="http://cayenne.apache.org/schema/3.0/modelMap"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://cayenne.apache.org/schema/3.0/modelMap http://cayenne.apache.org/schema/3.0/modelMap.xsd"
  project-version="3.0.0.1">
	<property name="defaultPackage" value="com.meritcharge.entity.station.cayenne"/>
	<property name="defaultSuperclass" value="com.meritcharge.entity.Entity"/>
	<property name="defaultClientPackage" value="com.meritcharge.entity"/>
	<property name="defaultClientSuperclass" value="com.meritcharge.entity.RemoteEntity"/>
	<db-entity name="Card">
		<db-attribute name="authorization" type="VARCHAR" length="8"/>
		<db-attribute name="cardAcceptorIdentification" type="VARCHAR" length="512"/>
		<db-attribute name="cardAcceptorTerminalIdentification" type="VARCHAR" length="512"/>
		<db-attribute name="creationDate" type="TIMESTAMP" isMandatory="true"/>
		<db-attribute name="error" type="VARCHAR" length="512"/>
		<db-attribute name="financialTransactionAdvice" type="VARCHAR" length="8"/>
		<db-attribute name="id" type="INTEGER" isPrimaryKey="true" isGenerated="true" isMandatory="true"/>
		<db-attribute name="identification" type="VARCHAR" isMandatory="true" length="512"/>
		<db-attribute name="merchantLocation" type="VARCHAR" length="512"/>
		<db-attribute name="merchantName" type="VARCHAR" length="512"/>
		<db-attribute name="merchantPhoneNumber" type="VARCHAR" length="512"/>
		<db-attribute name="retrievalReferenceNumber" type="VARCHAR" length="512"/>
		<db-attribute name="sale" type="VARCHAR" length="128"/>
		<db-attribute name="serviceID" type="INTEGER"/>
		<db-attribute name="truncatedPersonalAccountNumber" type="VARCHAR" length="4"/>
	</db-entity>
	<db-entity name="Price">
		<db-attribute name="creationDate" type="TIMESTAMP" isMandatory="true"/>
		<db-attribute name="currencyCode" type="INTEGER"/>
		<db-attribute name="currencySymbol" type="VARCHAR" length="3"/>
		<db-attribute name="id" type="INTEGER" isPrimaryKey="true" isGenerated="true" isMandatory="true"/>
		<db-attribute name="unit" type="VARCHAR" length="16"/>
		<db-attribute name="value" type="VARCHAR" length="64"/>
	</db-entity>
	<db-entity name="Product">
		<db-attribute name="creationDate" type="TIMESTAMP" isMandatory="true"/>
		<db-attribute name="elapsedSeconds" type="VARCHAR" length="64"/>
		<db-attribute name="id" type="INTEGER" isPrimaryKey="true" isGenerated="true" isMandatory="true"/>
		<db-attribute name="quantity" type="VARCHAR" length="64"/>
		<db-attribute name="unit" type="VARCHAR" length="64"/>
	</db-entity>
	<db-entity name="Service">
		<db-attribute name="creationDate" type="TIMESTAMP" isMandatory="true"/>
		<db-attribute name="endDate" type="TIMESTAMP"/>
		<db-attribute name="energyID" type="INTEGER"/>
		<db-attribute name="id" type="INTEGER" isPrimaryKey="true" isGenerated="true" isMandatory="true"/>
		<db-attribute name="identification" type="VARCHAR" length="15"/>
		<db-attribute name="priceID" type="INTEGER"/>
		<db-attribute name="startDate" type="TIMESTAMP"/>
	</db-entity>
	<db-entity name="Station">
		<db-attribute name="id" type="INTEGER" isPrimaryKey="true" isGenerated="true" isMandatory="true"/>
		<db-attribute name="startDate" type="TIMESTAMP"/>
		<db-attribute name="version" type="VARCHAR" length="64"/>
	</db-entity>
	<obj-entity name="Card" className="com.meritcharge.entity.station.cayenne.Card" clientClassName="com.meritcharge.entity.station.payment.client.Card" dbEntityName="Card" superClassName="com.meritcharge.entity.Entity" clientSuperClassName="com.meritcharge.entity.RemoteEntity">
		<obj-attribute name="authorization" type="java.lang.String" db-attribute-path="authorization"/>
		<obj-attribute name="cardAcceptorIdentification" type="java.lang.String" db-attribute-path="cardAcceptorIdentification"/>
		<obj-attribute name="cardAcceptorTerminalIdentification" type="java.lang.String" db-attribute-path="cardAcceptorTerminalIdentification"/>
		<obj-attribute name="creationDate" type="java.util.Date" db-attribute-path="creationDate"/>
		<obj-attribute name="error" type="java.lang.String" db-attribute-path="error"/>
		<obj-attribute name="financialTransactionAdvice" type="java.lang.String" db-attribute-path="financialTransactionAdvice"/>
		<obj-attribute name="identification" type="java.lang.String" db-attribute-path="identification"/>
		<obj-attribute name="merchantLocation" type="java.lang.String" db-attribute-path="merchantLocation"/>
		<obj-attribute name="merchantName" type="java.lang.String" db-attribute-path="merchantName"/>
		<obj-attribute name="merchantPhoneNumber" type="java.lang.String" db-attribute-path="merchantPhoneNumber"/>
		<obj-attribute name="retrievalReferenceNumber" type="java.lang.String" db-attribute-path="retrievalReferenceNumber"/>
		<obj-attribute name="sale" type="java.lang.String" db-attribute-path="sale"/>
		<obj-attribute name="truncatedPersonalAccountNumber" type="java.lang.String" db-attribute-path="truncatedPersonalAccountNumber"/>
	</obj-entity>
	<obj-entity name="Price" className="com.meritcharge.entity.station.cayenne.Price" clientClassName="com.meritcharge.entity.station.payment.client.Price" dbEntityName="Price" superClassName="com.meritcharge.entity.Entity" clientSuperClassName="com.meritcharge.entity.RemoteEntity">
		<obj-attribute name="creationDate" type="java.util.Date" db-attribute-path="creationDate"/>
		<obj-attribute name="currencyCode" type="java.lang.Integer" db-attribute-path="currencyCode"/>
		<obj-attribute name="currencySymbol" type="java.lang.String" db-attribute-path="currencySymbol"/>
		<obj-attribute name="unit" type="java.lang.String" db-attribute-path="unit"/>
		<obj-attribute name="value" type="java.lang.String" db-attribute-path="value"/>
	</obj-entity>
	<obj-entity name="Product" className="com.meritcharge.entity.station.cayenne.Product" clientClassName="com.meritcharge.entity.station.client.Product" dbEntityName="Product" superClassName="com.meritcharge.entity.Entity" clientSuperClassName="com.meritcharge.entity.RemoteEntity">
		<obj-attribute name="creationDate" type="java.util.Date" db-attribute-path="creationDate"/>
		<obj-attribute name="elapsedSeconds" type="java.lang.String" db-attribute-path="elapsedSeconds"/>
		<obj-attribute name="quantity" type="java.lang.String" db-attribute-path="quantity"/>
		<obj-attribute name="unit" type="java.lang.String" db-attribute-path="unit"/>
	</obj-entity>
	<obj-entity name="Service" className="com.meritcharge.entity.station.cayenne.Service" clientClassName="com.meritcharge.entity.station.client.Service" dbEntityName="Service" superClassName="com.meritcharge.entity.Entity" clientSuperClassName="com.meritcharge.entity.RemoteEntity">
		<obj-attribute name="creationDate" type="java.util.Date" db-attribute-path="creationDate"/>
		<obj-attribute name="endDate" type="java.util.Date" db-attribute-path="endDate"/>
		<obj-attribute name="identification" type="java.lang.String" db-attribute-path="identification"/>
		<obj-attribute name="startDate" type="java.util.Date" db-attribute-path="startDate"/>
	</obj-entity>
	<obj-entity name="Station" serverOnly="true" className="com.meritcharge.entity.station.cayenne.Station" clientClassName="com.meritcharge.entity.Station" dbEntityName="Station" superClassName="com.meritcharge.entity.Entity" clientSuperClassName="com.meritcharge.entity.RemoteEntity">
		<obj-attribute name="startDate" type="java.util.Date" db-attribute-path="startDate"/>
		<obj-attribute name="version" type="java.lang.String" db-attribute-path="version"/>
	</obj-entity>
	<db-relationship name="service" source="Card" target="Service" toMany="false">
		<db-attribute-pair source="serviceID" target="id"/>
	</db-relationship>
	<db-relationship name="service" source="Price" target="Service" toMany="true">
		<db-attribute-pair source="id" target="priceID"/>
	</db-relationship>
	<db-relationship name="service" source="Product" target="Service" toMany="true">
		<db-attribute-pair source="id" target="energyID"/>
	</db-relationship>
	<db-relationship name="card" source="Service" target="Card" toMany="false">
		<db-attribute-pair source="id" target="serviceID"/>
	</db-relationship>
	<db-relationship name="price" source="Service" target="Price" toMany="false">
		<db-attribute-pair source="priceID" target="id"/>
	</db-relationship>
	<db-relationship name="product" source="Service" target="Product" toMany="false">
		<db-attribute-pair source="energyID" target="id"/>
	</db-relationship>
	<obj-relationship name="service" source="Card" target="Service" deleteRule="Cascade" db-relationship-path="service"/>
	<obj-relationship name="service" source="Price" target="Service" deleteRule="Nullify" db-relationship-path="service"/>
	<obj-relationship name="service" source="Product" target="Service" deleteRule="Nullify" db-relationship-path="service"/>
	<obj-relationship name="card" source="Service" target="Card" deleteRule="Nullify" db-relationship-path="card"/>
	<obj-relationship name="price" source="Service" target="Price" deleteRule="Cascade" db-relationship-path="price"/>
	<obj-relationship name="product" source="Service" target="Product" deleteRule="Cascade" db-relationship-path="product"/>
</data-map>
