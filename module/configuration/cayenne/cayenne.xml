<?xml version="1.0" encoding="utf-8"?>
<domains project-version="3.0.0.1">
<domain name="StationDomain">
	<map name="StationDomainMap" location="StationDomainMap.map.xml"/>

	<node name="StationDomainNode"
		 datasource="SQLiteStationDomainNode.driver.xml"
		 factory="org.apache.cayenne.conf.DriverDataSourceFactory"
		 schema-update-strategy="org.apache.cayenne.access.dbsync.CreateIfNoSchemaStrategy">
			<map-ref name="StationDomainMap"/>
	 </node>
